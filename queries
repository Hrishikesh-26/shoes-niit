create table user
(
userid int primary key auto_increment,
username varchar(50),
email varchar(5000),
password varchar(50),
role varchar(60)
);

drop table products
create table products
(
productid int primary key auto_increment,
name varchar(60),
price float,
category varchar(70),
brand varchar(90),
size int,
colour varchar(70),
quantity int,
description varchar(5000),
image longblob
);

select * from user

select * from products